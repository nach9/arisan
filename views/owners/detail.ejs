<!DOCTYPE html>
<html lang="en">
<% include ../partials/head%>
<body class="fixed-nav sticky-footer bg-dark" id="page-top">
  <% include ../partials/nav%>
  <div class="content-wrapper">
  	<!-- content here -->
    <div class="container-fluid">
      <!-- Breadcrumbs-->
      <ol class="breadcrumb">
        <li class="breadcrumb-item">
          <a href="/user">User</a>
        </li>
        <li class="breadcrumb-item">
          <a href="/owner">Owner</a>
        </li>
        <li class="breadcrumb-item active">Detail</li>
      </ol>
      <!-- Example DataTables Card-->
      <div class="card mb-3">
      	<div class="card-header"><i class="fa fa-table"></i> Group Detail</div>
        <div class="card-body">
          <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
              <thead>
                <tr>
                  <th>ID</th>
                  <th>Name</th>
                  <th>Phone</th>
                  <th>Email</th>
                  <th>Status</th>
                  <th>Actions</th>
                </tr>
              </thead>
              <tbody>
                <% rows.Users.forEach((row) => { %>
                  <tr>
                    <td><%= row.id %></td>
                    <td><%= row.getFullName() %></td>
                    <td><%= row.phone %></td>
                    <td><%= row.email %></td>
                    <td></td>
                    <td>
                      <a href="/owner/detail/<%= rows.id %>/<%= row.id %>/delete">Delete</a>
                    </td>
                  </tr>
                <% }) %>
              </tbody>
            </table>
          </div>
        </div>
        <div class="card-footer small text-muted"><a href="/owner/detail/<%= rows.id %>/addmember">Add Member</a></div>
      </div>
    </div>
    <!-- /content -->
	</div>
    <% include ../partials/footer%>
  </div>
</body>
</html>